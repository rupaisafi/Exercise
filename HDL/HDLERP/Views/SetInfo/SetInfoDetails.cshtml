
@Scripts.Render("~/Scripts/HDL/SetInfo/SetInfoDetails.js")

<div class="row">
    <div class="col-md-12 col-xs-12">
        <div class="bs-example">
            <div class="well well-sm">Set Entry Form</div>
            <div class="col-md-8">
                <div class="form-horizontal">
                    <input type="hidden" id="hdnSetId" value="0"/>

                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtSetNo">Set No:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" id="txtSetNo" name="txtSetNo" required validationmessage="Required" placeholder="" style="width: 100%; height: 28px;" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtWarpDate">Warp Date:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" placeholder="" style="width: 100%; height: 28px;" name="txtWarpDate" id="txtWarpDate" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtLength">Length:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" placeholder="" style="width: 100%; height: 28px;" name="txtLength" id="txtLength" />
                        </div>
                    </div>
                    
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtEndsPerBeam">Ends Per Beam:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" placeholder="" style="width: 100%; height: 28px;" name="txtEndsPerBeam" id="txtEndsPerBeam" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtTotalBeam">Total Beam:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" placeholder="" style="width: 100%; height: 28px;" name="txtTotalBeam" id="txtTotalBeam" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtTotalEnds">Total Ends:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" placeholder="" style="width: 100%; height: 28px;" name="txtTotalEnds" id="txtTotalEnds"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtDrumPreasure">Drum Preasure:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" placeholder="" style="width: 100%; height: 28px;" name="txtDrumPreasure" id="txtDrumPreasure" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtYarnTension">Yarn Tension:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" class="form-control" placeholder="" style="width: 100%; height: 28px;" name="txtYarnTension" id="txtYarnTension"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtBeamCreel">Beam Creel:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" placeholder="" style="width: 100%; height: 28px;" name="txtBeamCreel" id="txtBeamCreel" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtMachineNo">Machine No:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" class="form-control" placeholder="" style="width: 100%; height: 28px;" name="txtMachineNo" id="txtMachineNo"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="cmbProductionTypeCode">Production Type:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" placeholder="" style="width: 100%; height: 28px;" name="cmbProductionTypeCode" id="cmbProductionTypeCode" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="cmbSetStatusCode">Status:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" placeholder="" style="width: 100%; height: 28px;" name="cmbSetStatusCode" id="cmbSetStatusCode" />
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtRemarks">Remarks:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" class="form-control" placeholder="" style="width: 100%; height: 28px;" name="txtRemarks" id="txtRemarks" />
                        </div>
                    </div>

                </div>
            </div>

            <div class="row">
                <div class="spacer5"></div>
                <div class="col-md-12 col-xs-12">
                    <ul class="list-group ">
                        <li class="list-group-item centerAlign">
                            <button id="btnSave" class="btn btn-success" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Save</button>
                            <button id="btnClear" class="btn btn-danger" type="button"><span class="glyphicon glyphicon-repeat"></span> Clear</button>
                            @*<button id="btnClose" class="btn btn-danger" type="button"><span class="glyphicon glyphicon-eject"></span> Close</button>*@
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
