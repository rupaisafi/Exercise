@Scripts.Render("~/Scripts/HDL/DyeingProduction/DyeingProductionSummary.js")

<div class="row">
    <div class="col-md-12 col-xs-12">
        <div class="bs-example">

            <div class="well well-sm">
               Dyeing Production 
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-3 ">
                        <button id="btnAddNew" class="btn btn-primary" type="button"><span class="glyphicon glyphicon-plus-sign"></span> Add New</button>
                    </div>
                    <div class="col-md-9">
                        <div class="col-md-4">
                            <label class="col-md-4  control-label" for="txtSearchDateFrom">From Date:</label>
                            <div class="col-md-8 input-group">
                                <input type="text" placeholder="Warping Date" name="txtSearchDateFrom" style="width: 100%; height: 24px;" id="txtSearchDateFrom">
                            </div>
                        </div>
                        <div class="col-md-4 ">
                            <label class="col-md-4  control-label" for="txtSearchDateTo">To Date:</label>
                            <div class="col-md-8 input-group">
                                <input type="text" placeholder="Warping Date" name="txtSearchDateTo" style="width: 100%; height: 24px;" id="txtSearchDateTo">
                            </div>
                        </div>
                        <div class="col-md-4 ">
                            <button id="btnSearchWarping" class="btn btn-sm btn-info" style="height: 26px;"><span class="glyphicon glyphicon-search"></span></button>
                        </div>
                    </div>
                </div>
                <div class="spacer4"></div>
              
                <div id="grdDyeingProductionSummary">
                </div>
            </div>
        </div>
    </div>
</div>

