@Scripts.Render("~/Scripts/HDL/DyeingConsumption/DyeingConsumptionSummary.js")

<div class="row">
    <div class="col-md-12 col-xs-12">
        <div class="bs-example">

            <div class="well well-sm">
                Dyeing Consumption
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-3 ">
                        <button id="dcsNew" class="btn btn-primary" type="button">
                            <span class="glyphicon glyphicon-plus-sign"></span>
                            Add New
                        </button>
                    </div>
                    <div class="col-md-9">
                        <div class="col-md-4">
                            <label class="col-md-4  control-label" for="dcsDateFrom">From Date:</label>
                            <div class="col-md-8 input-group">
                                <input type="text" placeholder="From Date" name="dcsDateFrom" style="width: 100%; height: 24px;" id="dcsDateFrom">
                            </div>
                        </div>
                        <div class="col-md-4 ">
                            <label class="col-md-4  control-label" for="dcsDateTo">To Date:</label>
                            <div class="col-md-8 input-group">
                                <input type="text" placeholder="To Date" name="dcsDateTo" style="width: 100%; height: 24px;" id="dcsDateTo">
                            </div>
                        </div>
                        <div class="col-md-4 ">
                            <button id="dcsSearch" class="btn btn-sm btn-info" style="height: 26px;"><span class="glyphicon glyphicon-search"></span></button>
                        </div>
                    </div>
                </div>
                <div class="spacer4"></div>
                <div id="dcsGrid">
                </div>
            </div>
        </div>
    </div>
</div>
