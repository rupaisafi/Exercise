@Scripts.Render("~/Scripts/HDL/MIssueInfo/MIssueDetailsPopup.js")

<div class="row">
    <div class="col-md-12 col-xs-12" id="MIssueDetailsWindow">
        <div class="well well-sm" id="lblTitle">YARN INFORMATION ENTRY</div>
        <div class="bs-example">
            <div class="row">
                <div class="col-md-4 col-xs-4">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="cmbItem">Item Name:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" placeholder="" style="width: 100%; height: 28px;" name="cmbItem" id="cmbItem" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="txtUnit">Unit:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" class="form-control" style="width: 100%; height: 28px;" name="txtUnit" id="txtUnit" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="cmbLotNo">Lot No:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" style="width: 100%; height: 28px;" name="cmbLotNo" id="cmbLotNo" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="txtInvNo">Invoice No:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" class="form-control" name="txtInvNo" style="width: 100%; height: 28px;" id="txtInvNo">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="cmbLcNo">LC No:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" name="cmbLcNo" style="width: 100%; height: 28px;" id="cmbLcNo" required validationmessage="Required">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="cmbSupplier">Supplier:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" name="cmbSupplier" style="width: 100%; height: 28px;" id="cmbSupplier">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="cmbImportType">Import Type:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" name="cmbImportType" style="width: 100%; height: 28px;" id="cmbImportType">
                            </div>
                        </div>





                    </div>
                </div>
                <div class="col-md-4 col-xs-4">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="cmbYarn">Yarn Code:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" style="width: 100%; height: 28px;" name="cmbYarn" id="cmbYarn" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="txtMIssueQty">Qnty:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" style="width: 100%; height: 28px;" name="txtMIssueQty" id="txtMIssueQty" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="txtMIssueRate">Rate ($):</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" style="width: 100%; height: 28px;" name="txtMIssueRate" id="txtMIssueRate" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="txtMIssueAmt">Value ($):</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" style="width: 100%; height: 28px;" name="txtMIssueAmt" id="txtMIssueAmt" readonly="readonly" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="cmbCurrency">Currency:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" style="width: 100%; height: 28px;" name="cmbCurrency" id="cmbCurrency" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="txtRateTaka">Rate (Taka):</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" style="width: 100%; height: 28px;" name="txtRateTaka" id="txtRateTaka" required validationmessage="Required" />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="txtValueTaka">Value (Taka):</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" style="width: 100%; height: 28px;" name="txtValueTaka" id="txtValueTaka" readonly="readonly" />
                            </div>
                        </div>

                    </div>
                </div>
                <div class="col-md-4 col-xs-4">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="cmbSetNo">Set No:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" style="width: 100%; height: 28px;" name="cmbSetNo" id="cmbSetNo" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="cmbCottonType">Cotton Type:</label>
                            <div class="col-md-7 col-xs-7 ">
                                <input type="text" style="width: 100%; height: 28px;" name="cmbCottonType" id="cmbCottonType" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="txtRemarks">Remarks:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" class="form-control" style="width: 100%; height: 28px;" name="txtRemarks" id="txtRemarks" />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="cmbGroupName">Group Name:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" style="width: 100%; height: 28px;" name="cmbGroupName" id="cmbGroupName" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="spacer5"></div>
    <div class="col-md-12 col-xs-12">
        <ul class="list-group ">
            <li class="list-group-item centerAlign">
                <button id="btnAddToList" class="btn btn-success" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> AddToList</button>
                <button id="btnUpdateMIssueDetails" class="btn btn-success displayNone" type="button"><span class="glyphicon glyphicon-download-alt"></span> Update List</button>
                <button id="btnClearMIssueDetails" class="btn btn-info" type="button"><span class="glyphicon glyphicon-repeat"></span> Clear</button>
                <button id="btnCloseMIssueDetails" class="btn btn-danger" type="button"><span class="glyphicon glyphicon-remove"></span> Close</button>
            </li>
        </ul>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <label class="col-md-12 col-xs-12 control-label centerAlign" id="lblMessage"></label>
    </div>
</div>

