
@{
    ViewBag.Title = "Index";
}

@Styles.Render("~/Content/HDL/StickyEnd.css")

<div class="divWraper" id="divWraper">
    <div class="divSummaryWraper">
        <div class="bs-example">
            <div class="well well-sm">
                Sticky Ends Summary
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-3 ">
                        <button class="btn btn-primary btn-add-summary" type="button"><span class="glyphicon glyphicon-plus-sign"></span> Add New</button>
                    </div>
                    <div class="col-md-9">
                        <div class="col-md-4">
                            <label class="col-md-4  control-label" for="fromDate">From Date:</label>
                            <div class="col-md-8 input-group">
                                <input id="fromDate" placeholder="From Date" style="width: 100%; height: 24px;">
                            </div>
                        </div>
                        <div class="col-md-4 ">
                            <label class="col-md-4  control-label" for="toDate">To Date:</label>
                            <div class="col-md-8 input-group">
                                <input id="toDate" placeholder="To Date" style="width: 100%; height: 24px;">
                            </div>
                        </div>
                        <div class="col-md-4 ">
                            <button id="btnSearchWarping" class="btn btn-sm btn-info" style="height: 26px;"><span class="glyphicon glyphicon-search"></span></button>
                        </div>
                    </div>
                </div>
                <div class="spacer4"></div>
                <div id="divSummaryGrid">
                </div>
            </div>
        </div>
    </div>
    <div class="divDetailWraper displayNone">
        <div class="bs-example">
            <div class="well well-sm">
                Sticky Ends Entry Form
            </div>
            <br />
            <div class="collapse in" id="demo">
                <div class="col-md-3">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="semDate">Date:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="semID" type="hidden" value="" />
                                <input id="semDate" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="semDeptCode">Department:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="semDeptCode" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="semUnitCode">Unit:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="semUnitCode" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="semRemark">Remarks:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="semRemark" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="well well-sm">
                Sticky Ends Entry Form
            </div>
            <br />
            <div class="clearfix"></div>
            <div class="collapse in" id="demo">
                <div class="col-md-3">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedLoom">Loom:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedID" type="hidden" value="" />
                                <input id="sedLoom" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedSetNo">SetNo:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedSetNo" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="xxxxxx">SSNo:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedSSNo" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedBeamNo">BeamNo:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedBeamNo" style="width: 100%; height: 24px;" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedBeamLength">SizingLen:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedBeamLength" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedStyleNo">StyleNo:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedStyleNo" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedGConstruction">GConstruction:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedGConstruction" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedTEnds">TEnds:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedTEnds" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedKnott">Knott:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedKnott" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedByPass1stDay">ByPass1stDay:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedByPass1stDay" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedByPass2ndDay">ByPass2ndDay:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedByPass2ndDay" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedSizingW">SizingW:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedSizingW" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedSizingD">SizingD:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedSizingD" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedShortEnds">ShortEnds:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedShortEnds" style="width: 100%; height: 24px;" readonly="readonly" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedRemarks">Remarks:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedRemarks" style="width: 100%; height: 24px;" required validationmessage="Required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="sedSID">SID:</label>
                            <div class="col-md-7 col-xs-7">
                                <input id="sedSID" style="width: 100%; height: 24px;" readonly="readonly" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="row">
                <div class="col-md-12 col-xs-12">
                    <ul class="list-group ">
                        <li class="list-group-item rightAlign">
                            <button class="btn btn-sm btn-success btn-data-entry-detail-save" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Save</button>
                            <button class="btn btn-sm btn-info btn-data-entry-detail-clear" type="button"><span class="glyphicon glyphicon-repeat"></span> New</button>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="panel-body">

                <div class="row">
                    <div class="col-md-12">
                        <div class="divDetailGrid">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 col-xs-12">
                        <ul class="list-group ">
                            <li class="list-group-item centerAlign">
                                <button class="btn btn-success btn-data-entry-save" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Save</button>
                                <button class="btn btn-info btn-data-entry-clear" type="button"><span class="glyphicon glyphicon-repeat"></span> New</button>
                                <button class="btn btn-danger btn-data-entry-close" type="button"><span class="glyphicon glyphicon-remove"></span> Close</button>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section scripts{
    @Scripts.Render("~/Scripts/HDL/StickyEnd/index.js")
}
