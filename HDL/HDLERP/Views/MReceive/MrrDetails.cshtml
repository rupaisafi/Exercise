@Scripts.Render("~/Scripts/HDL/MrrInfo/MrrDetails.js")
@Scripts.Render("~/Scripts/HDL/MrrInfo/MrrDetailsSummary.js")

<div class="row">
    <div class="col-md-12 col-xs-12">
        <div class="well well-sm">Material Receive Details</div>
        <div class="bs-example" id="divMrrDetails">

            <div class="col-md-3">
                <div class="form-horizontal">
                    <input type="hidden" id="hdnMrrId" value="0" />
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="cmbCompany">Company:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" style="width: 100%; height: 28px;" name="cmbCompany" id="cmbCompany" required validationmessage="Required" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtMrrDate">Receive Date:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" placeholder="" style="width: 100%; height: 28px;" name="txtMrrDate" id="txtMrrDate" required validationmessage="Required" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtMrrNo">MRR No.:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" name="txtMrrNo" class="form-control" style="width: 100%; height: 28px;" id="txtMrrNo" required validationmessage="Required">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="cmbLcNo">LC No:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" name="cmbLcNo" style="width: 100%; height: 28px;" id="cmbLcNo" required validationmessage="Required">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtLcDate">LC Date:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" name="txtLcDate" style="width: 100%; height: 28px;" id="txtLcDate">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="cmbSupplier">Supplier:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" name="cmbSupplier" style="width: 100%; height: 28px;" id="cmbSupplier">
                        </div>
                    </div>
                   
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtInvNo">Invoice No:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" class="form-control" name="txtInvNo" style="width: 100%; height: 28px;" id="txtInvNo">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-horizontal">
                
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtInvDate">Invoice Date:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" name="txtInvDate" style="width: 100%; height: 28px;" id="txtInvDate">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtPiNo">P.I.No:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" class="form-control" name="txtPiNo" style="width: 100%; height: 28px;" id="txtPiNo">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtPIDate">P.I.Date:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" name="txtPIDate" style="width: 100%; height: 28px;" id="txtPIDate">
                        </div>
                    </div>
              
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtIpNo">I.P.No:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" class="form-control" name="txtIpNo" style="width: 100%; height: 28px;" id="txtIpNo">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtIPDate">I.P.Date:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" name="txtIPDate" style="width: 100%; height: 28px;" id="txtIPDate">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="cmbImportType">Import Type:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" name="cmbImportType" style="width: 100%; height: 28px;" id="cmbImportType">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtRemarks1">Remarks:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" class="form-control" name="txtRemarks1" style="width: 100%; height: 28px;" id="txtRemarks1">
                        </div>
                    </div>

                </div>
            </div>
            <div class="col-md-3">
                <div class="form-horizontal">
                   
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtBlNo">B.L.No:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" class="form-control" name="txtBlNo" style="width: 100%; height: 28px;" id="txtBlNo">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtBlDate">B.L.Date:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" name="txtBlDate" style="width: 100%; height: 28px;" id="txtBlDate">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtBeNo">B.E.No:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" class="form-control" name="txtBeNo" style="width: 100%; height: 28px;" id="txtBeNo">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtBeDate">B.E.Date:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" name="txtBeDate" style="width: 100%; height: 28px;" id="txtBeDate">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtInvValue">Invoice Value:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" class="form-control" name="txtInvValue" style="width: 100%; height: 28px;" id="txtInvValue">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="txtTruckNo">Truck No:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" class="form-control" name="txtTruckNo" style="width: 100%; height: 28px;" id="txtTruckNo">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 col-xs-5 control-label" for="cmbPartial">Partial:</label>
                        <div class="col-md-7 col-xs-7">
                            <input type="text" name="cmbPartial" style="width: 100%; height: 28px;" id="cmbPartial">
                        </div>
                    </div>
                  
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-horizontal">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="txtCertificateNo">Certificate No:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" class="form-control" name="txtCertificateNo" style="width: 100%; height: 28px;" id="txtCertificateNo">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="txtChallanNo">Challan No:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" class="form-control" name="txtChallanNo" style="width: 100%; height: 28px;" id="txtChallanNo">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="txtChallanDate">Challan Date:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" name="txtChallanDate" style="width: 100%; height: 28px;" id="txtChallanDate">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-5 col-xs-5 control-label" for="cmbPayment">Payment:</label>
                            <div class="col-md-7 col-xs-7">
                                <input type="text" name="cmbPayment" style="width: 100%; height: 28px;" id="cmbPayment">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 col-xs-12">
                    <div class="panel-body">
                        <button id="btnAddMrrDetails" class="btn btn-primary" type="button">Add Details</button>
                        <div id="grdMrrDetailsSummary">
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="spacer5"></div>
                <div class="col-md-12 col-xs-12">
                    <ul class="list-group ">
                        <li class="list-group-item centerAlign">
                            <button id="btnSave" class="btn btn-success" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Save</button>
                            <button id="btnClear" class="btn btn-info" type="button"><span class="glyphicon glyphicon-repeat"></span> Clear</button>
                            <button id="btnClose" class="btn btn-danger" type="button"><span class="glyphicon glyphicon-remove"></span> Close</button>
                        </li>
                    </ul>
                </div>
            </div>
        </div>

    </div>
</div>




